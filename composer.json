{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "rnr1721/depthnet",
    "type": "project",
    "description": "Laravel-based autonomous AI agent platform with cyclic thinking, persistent memory and real-time code execution",
    "homepage": "https://github.com/rnr1721",
    "keywords": [
        "laravel",
        "ai",
        "agent",
        "artificial-intelligence",
        "digital-consciousness",
        "autonomous-ai",
        "experimental"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "tightenco/ziggy": "^2.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate || true",
            "@php artisan ide-helper:models --nowrite || true",
            "@php artisan ide-helper:meta || true"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php -r \"file_exists('stubs/htaccess.stub') && copy('stubs/htaccess.stub', 'public/.htaccess');\"",
            "@php -r \"file_exists('stubs/robots.stub') && copy('stubs/robots.stub', 'public/robots.txt');\"",
            "@php artisan storage:link --graceful",
            "@php artisan migrate --graceful --ansi",
            "@php artisan db:seed --graceful",
            "@php artisan ziggy:generate --graceful",
            "@composer check-node || (echo 'Node.js required! Install from https://nodejs.org' && exit 1)",
            "npm install",
            "npm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "check-node": [
            "@php -r \"exec('node --version 2>&1', $output, $code); if($code !== 0) { echo 'Node.js required! Install from https://nodejs.org'; exit(1); }\""
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}